


<div class="container">
  <div class="heading">
    <h1 class="fa-7x fw-bold"> My feed</h1>
  </div>

  <% @tois.each do |toi| %>
    <div class="card flex-row justify-content-between">
      <div class="infos d-flex">
        <div class="">
          <div>
            <h3 class="text-dark fw-bold">  <%= toi.title %> </h3>
              <% toi.followed_users_posts(current_user).each do |post| %>
              <div class="d-flex">
                <img class="avatar d-block" src="https://static.vecteezy.com/system/resources/thumbnails/005/129/844/small/profile-user-icon-isolated-on-white-background-eps10-free-vector.jpg" alt="">
              </div>
                <% end %>
                   </div>
        </div>
      </div>
      <img class="photo" src="https://www.originaltravel.co.uk/travel-blog/ShowPhoto/3521/0" alt="">
    </div>
  <% end %>
</div>
